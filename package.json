{
  "name": "k6-docker",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --platform=node --format=cjs --target=node20 --outfile=dist/server.cjs && esbuild src/loadTest.ts --bundle --platform=neutral --format=esm --target=es2020 --outfile=dist/test.js --external:k6 --external:k6/*",
    "start": "node dist/server.cjs"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/k6": "^1.3.1",
    "@types/node": "^24.10.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
